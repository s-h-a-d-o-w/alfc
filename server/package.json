{
  "name": "server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && ncc build index.ts",
    "build:cpuoc": "csc /reference:native\\windows\\IntelOverclockingSDK.dll /t:library /out:native\\windows\\CPUOC.dll native\\windows\\CPUOC.cs",
    "build:wmiapi": "csc /t:library /out:native\\windows\\WmiAPI.dll native\\windows\\WmiAPI.cs",
    "lint": "eslint --ignore-path .gitignore --ext=.ts,.tsx .",
    "start": "nodemon --ext ts,dll",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "debug:acpi": "ts-node --transpileOnly native\\windows\\acpi.ts",
    "debug:oc": "pnpm build:cpuoc && ts-node --transpileOnly native\\windows\\cpuoc.ts"
  },
  "dependencies": {
    "edge-js": "18.4.3",
    "express": "4.17.1",
    "is-elevated": "3.0.0",
    "json-stringify-pretty-compact": "2.0.0",
    "lodash": "4.17.21",
    "ws": "7.3.1"
  },
  "devDependencies": {
    "@types/express": "4.17.7",
    "@types/node": "14.0.27",
    "@types/ws": "7.2.6",
    "@vercel/ncc": "0.38.1",
    "cross-env": "7.0.2",
    "kill-port": "1.6.1",
    "nodemon": "2.0.4",
    "rimraf": "3.0.2",
    "ts-node": "10.7.0"
  }
}
