{
  "name": "alfc",
  "version": "1.1.2",
  "author": "Andreas Opferkuch <andreas.opferkuch@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "^18"
  },
  "private": true,
  "scripts": {
    "install": "pnpm install:bootstrap && pnpm install:frontend && pnpm install:server",
    "install:bootstrap": "cd bootstrap && pnpm install",
    "install:frontend": "cd frontend && pnpm install",
    "install:server": "cd server && pnpm install",
    "lint": "eslint .",
    "precommit":"concurrently \"pnpm type-check\" \"lint-staged\"",
    "prepare": "husky",
    "start": "concurrently \"cd server && pnpm start\" \"cd frontend && pnpm start\"",
    "start:prod": "cross-env NODE_ENV=production node server/build",
    "test": "vitest",
    "type-check": "concurrently \"tsc -P ./frontend/tsconfig.json\" \"tsc --noEmit -P ./server/tsconfig.json\""
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash": "4.14.159",
    "@types/ws": "7.2.6",
    "@typescript-eslint/eslint-plugin": "8.19.0",
    "@typescript-eslint/parser": "8.19.0",
    "concurrently": "5.2.0",
    "cross-env": "7.0.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "husky": "^9.1.6",
    "kill-port": "1.6.1",
    "lint-staged": "^15.3.0",
    "prettier": "3.4.2",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.19.0",
    "vitest": "2.1.8"
  }
}
