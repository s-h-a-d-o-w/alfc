    "open": "^10.1.0"
  },
  "devDependencies": {
    "@types/node-abi": "^3.0.3",
    "@vercel/ncc": "0.38.3",
    "esbuild": "^0.24.2",
    "glob": "^11.0.1",
    "node-abi": "^3.73.0",
    "rimraf": "6.0.1",
    "tsx": "^4.19.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "@vscode/sudo-prompt": "patches/@vscode__sudo-prompt.patch"
    }
  }
